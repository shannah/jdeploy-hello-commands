{
    "bin": {"jdeploy-hello-commands": "jdeploy-bundle/jdeploy.js"},
    "author": "Steve Hannah",
    "description": "jDeploy app with commands",
    "main": "index.js",
    "preferGlobal": true,
    "repository": "",
    "version": "1.0.0",
    "jdeploy": {
        "jdk": false,
        "fallbackToUniversal": false,
        "buildCommand": [
            "./mvnw",
            "package"
        ],
        "publishTargets": [{
            "name": "github: shannah/jdeploy-hello-commands",
            "type": "GITHUB",
            "url": "shannah/jdeploy-hello-commands"
        }],
        "javaVersion": "21",
        "documentTypes": [],
        "jar": "target/jdeploy-hello-commands-1.0-SNAPSHOT.jar",
        "javafx": false,
        "title": "Hello Commands",
        "commands": {
            "hellosteve": {
                "args": ["Hello Steve!"],
                "description": "Says hello to Steve"
            },
            "goodbyesteve": {
                "args": ["Goodbye Steve!"],
                "description": "Says goodbye to Steve"
            }
        },
        "downloadPage": {"platforms": ["default"]},
        "platformBundlesEnabled": false
    },
    "dependencies": {
        "command-exists-promise": "^2.0.2",
        "node-fetch": "2.6.7",
        "tar": "^4.4.8",
        "yauzl": "^2.10.0",
        "shelljs": "^0.8.4"
    },
    "license": "ISC",
    "name": "jdeploy-hello-commands",
    "files": ["jdeploy-bundle"],
    "scripts": {"test": "echo \"Error: no test specified\" && exit 1"}
}